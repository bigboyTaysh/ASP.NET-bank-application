@model IEnumerable<BankApplication.ViewModels.CreationDateGroup>

<h3>@ViewBag.Message</h3>

<p></p>

<h2>Statystyki kont bankowych</h2>

<table class="table">
    <tr>
        <th>
            Data stworzenia
        </th>
        <th>
            Ilość kont
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CreationDate)
            </td>
            <td>
                @item.BankAccountsCount
            </td>
        </tr>
    }
</table>